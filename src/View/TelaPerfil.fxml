<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="717.0" prefWidth="1092.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.TelaPerfilController">
   <children>
      <AnchorPane prefHeight="717.0" prefWidth="275.0" style="-fx-background-color: blue;">
         <children>
            <Button fx:id="buttonAlterarSenha" layoutX="178.0" layoutY="632.0" mnemonicParsing="false" onAction="#alterarSenha" prefHeight="26.0" prefWidth="88.0" text="Alterar senha" />
            <Circle fx:id="fotoPerfil" fill="#eeeeee" layoutX="127.0" layoutY="96.0" radius="55.0" stroke="BLACK" strokeType="INSIDE" />
            <Label fx:id="cpfPerfil" layoutX="14.0" layoutY="232.0" prefHeight="54.0" prefWidth="260.0" text="CPF: " textFill="WHITE">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label fx:id="emailPerfil" layoutX="14.0" layoutY="205.0" prefHeight="54.0" prefWidth="208.0" text="Email:" textFill="#fffafa">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label fx:id="nomePerfil" layoutX="14.0" layoutY="178.0" prefHeight="54.0" prefWidth="226.0" text="Nome do Usuario: " textFill="WHITE">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
